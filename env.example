# ===========================================
# ADOPTLY PET LISTING - Environment Variables
# ===========================================
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# -------------------------------------------
# CLOUDINARY CONFIGURATION
# -------------------------------------------
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here
CLOUDINARY_PET_UPLOAD_PRESET=your_pet_preset_here
CLOUDINARY_USER_PROFILE_UPLOAD_PRESET=your_user_profile_preset_here

# -------------------------------------------
# MAPTILER CONFIGURATION
# -------------------------------------------
MAPTILER_API_KEY=your_maptiler_api_key_here

# -------------------------------------------
# GOOGLE APIS CONFIGURATION
# -------------------------------------------
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# -------------------------------------------
# FIREBASE CONFIGURATION (Web)
# -------------------------------------------
FIREBASE_WEB_API_KEY=your_firebase_web_api_key_here
FIREBASE_WEB_APP_ID=your_firebase_web_app_id_here
FIREBASE_WEB_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_WEB_PROJECT_ID=your_project_id_here
FIREBASE_WEB_AUTH_DOMAIN=your_auth_domain_here
FIREBASE_WEB_STORAGE_BUCKET=your_storage_bucket_here

# -------------------------------------------
# FIREBASE CONFIGURATION (Android)
# -------------------------------------------
FIREBASE_ANDROID_API_KEY=your_firebase_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_firebase_android_app_id_here

# -------------------------------------------
# FIREBASE CONFIGURATION (iOS)
# -------------------------------------------
FIREBASE_IOS_API_KEY=your_firebase_ios_api_key_here
FIREBASE_IOS_APP_ID=your_firebase_ios_app_id_here
FIREBASE_IOS_BUNDLE_ID=your_ios_bundle_id_here
